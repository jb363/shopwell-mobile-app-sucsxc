
{
  "expo": {
    "name": "ShopWell.ai",
    "slug": "shopwell-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.shopwell.ios",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "ShopWell.ai needs camera access for product photo search",
        "NSPhotoLibraryUsageDescription": "ShopWell.ai needs photo library access to select product images",
        "NSUserNotificationsUsageDescription": "ShopWell.ai needs notification access to send you updates about your shopping lists and products"
      },
      "associatedDomains": [
        "applinks:bda3e11e-68e8-45b3-9d3c-7c4fff44599c.lovableproject.com"
      ],
      "entitlements": {
        "com.apple.developer.associated-domains": [
          "applinks:bda3e11e-68e8-45b3-9d3c-7c4fff44599c.lovableproject.com"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#000000"
      },
      "package": "app.shopwell.android",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "bda3e11e-68e8-45b3-9d3c-7c4fff44599c.lovableproject.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "SEND",
          "category": ["DEFAULT"],
          "data": [
            { "mimeType": "text/plain" },
            { "mimeType": "image/*" }
          ]
        }
      ]
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/natively-dark.png",
          "color": "#000000",
          "sounds": [],
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "scheme": "shopwell",
    "experiments": {
      "typedRoutes": true
    }
  }
}

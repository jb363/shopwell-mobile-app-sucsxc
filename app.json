{
  "expo": {
    "name": "ShopWell Mobile",
    "slug": "ShopWell Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ShopWell.ai Logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/ShopWell.ai Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1D2E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.shopwell.ios",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "ShopWell.ai needs camera access for photo product search",
        "NSPhotoLibraryUsageDescription": "ShopWell.ai needs photo library access to search products from images"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ShopWell.ai Logo.png",
        "backgroundColor": "#1A1D2E"
      },
      "edgeToEdgeEnabled": true,
      "package": "app.shopwell.android"
    },
    "web": {
      "favicon": "./assets/images/c468683b-f963-43b7-9afe-fd73e23bad20.jpeg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "ShopWell Mobile",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
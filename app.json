{
  "expo": {
    "name": "ShopWell.ai Mobile",
    "slug": "shopwell-ai-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/shopwell-ai-app-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/shopwell-ai-app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1D2E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.shopwell.ios",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "ShopWell.ai needs camera access for photo product search",
        "NSPhotoLibraryUsageDescription": "ShopWell.ai needs photo library access to search products from images",
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "CFBundleAllowMixedLocalizations": true
      },
      "associatedDomains": [
        "applinks:shopwell.ai",
        "applinks:*.shopwell.ai"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/shopwell-ai-app-icon.png",
        "backgroundColor": "#1A1D2E"
      },
      "edgeToEdgeEnabled": true,
      "package": "app.shopwell.android",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.shopwell.ai",
              "pathPrefix": "/shared"
            },
            {
              "scheme": "https",
              "host": "*.shopwell.ai",
              "pathPrefix": "/lists"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "data": {
            "mimeType": "text/plain"
          },
          "category": [
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "data": {
            "mimeType": "image/*"
          },
          "category": [
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/9f690794-ae21-4465-a1f1-dd1f8d6a46e5.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "shopwell-ai-mobile",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2660e422-ff9f-4525-8ca0-080bf290cdf4"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2660e422-ff9f-4525-8ca0-080bf290cdf4"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
{
  "expo": {
    "name": "ShopWell.ai Mobile",
    "slug": "shopwell-mobile-app-sucsxc",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/shopwell-ai-app-icon.png",
    "scheme": "ShopWell.ai Mobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/shopwell-ai-app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shopwell.ai",
      "buildNumber": "16",
      "infoPlist": {
        "NSUserNotificationsUsageDescription": "ShopWell.ai uses notifications to alert you about personalized deals and store updates.",
        "NSLocationWhenInUseUsageDescription": "ShopWell.ai uses your location to provide personalized store notifications and deals.",
        "NSContactsUsageDescription": "ShopWell.ai needs access to your contacts to help you invite friends and share deals.",
        "NSMicrophoneUsageDescription": "ShopWell.ai needs microphone access for voice search and audio input features."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/shopwell-ai-app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.axiomstrategies.shopwellai",
      "versionCode": 12,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_CONTACTS",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/shopwell-ai-app-icon.png"
    },
    "plugins": [
      "expo-quick-actions",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
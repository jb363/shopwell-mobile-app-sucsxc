{
  "expo": {
    "name": "ShopWell.ai Mobile",
    "slug": "ShopWell.ai Mobile",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/shopwell-ai-app-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/shopwell-ai-app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1D2E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.shopwell.ios",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "ShopWell.ai needs camera access for photo product search",
        "NSPhotoLibraryUsageDescription": "ShopWell.ai needs photo library access to search products from images",
        "NSContactsUsageDescription": "ShopWell.ai needs access to your contacts to help you connect with friends and family on the platform",
        "NSMicrophoneUsageDescription": "ShopWell.ai needs microphone access for voice planning and audio features",
        "NSUserTrackingUsageDescription": "This allows us to provide you with personalized product recommendations and improve your shopping experience. Your data is used to show relevant content and offers.",
        "NSLocationWhenInUseUsageDescription": "ShopWell.ai needs your location to notify you when you're near stores with active shopping lists",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "ShopWell.ai needs your location in the background to send you reminders when you're near stores with active shopping lists",
        "UIBackgroundModes": [
          "remote-notification",
          "location"
        ],
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "shopwellai"
            ]
          }
        ]
      },
      "associatedDomains": [
        "applinks:shopwell.ai",
        "applinks:*.shopwell.ai"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/shopwell-ai-app-icon.png",
        "backgroundColor": "#1A1D2E"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.axiomstrategies.shopwellai",
      "versionCode": 7,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS",
        "READ_CONTACTS",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.shopwell.ai",
              "pathPrefix": "/shared"
            },
            {
              "scheme": "https",
              "host": "*.shopwell.ai",
              "pathPrefix": "/lists"
            },
            {
              "scheme": "shopwellai"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "data": [
            {
              "mimeType": "text/plain"
            }
          ],
          "category": [
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "data": [
            {
              "mimeType": "image/*"
            }
          ],
          "category": [
            "DEFAULT"
          ]
        },
        {
          "action": "SEND_MULTIPLE",
          "data": [
            {
              "mimeType": "image/*"
            }
          ],
          "category": [
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/9f690794-ae21-4465-a1f1-dd1f8d6a46e5.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This allows us to provide you with personalized product recommendations and improve your shopping experience. Your data is used to show relevant content and offers."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "ShopWell.ai needs your location in the background to send you reminders when you're near stores with active shopping lists",
          "locationWhenInUsePermission": "ShopWell.ai needs your location to notify you when you're near stores with active shopping lists",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-quick-actions",
        {
          "androidIcons": {
            "voice_planner": "./assets/images/shopwell-ai-app-icon.png",
            "product_search": "./assets/images/shopwell-ai-app-icon.png",
            "photo_search": "./assets/images/shopwell-ai-app-icon.png"
          }
        }
      ]
    ],
    "scheme": "shopwellai",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2660e422-ff9f-4525-8ca0-080bf290cdf4"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2660e422-ff9f-4525-8ca0-080bf290cdf4"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}